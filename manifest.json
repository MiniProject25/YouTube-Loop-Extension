{
    "manifest_version": 3,
    "name": "YouTube Loop Extension",
    "version": "1.0",
    "description": "Loop a portion of YouTube videos. Yup, that's it.",
    "permissions": [
        "tabCapture",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "background": {
        "service_worker": "script.js"
    },
    "action": {
        "default_popup": "input.html",
        "default_icon": {
            "16": "Icon.png",
            "48": "Icon.png",
            "128": "Icon.png"
        }
    }
}